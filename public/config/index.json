{
    "app": {
        "name": "DataTables Viewer",
        "version": "4.0.0",
        "description": "Professional researcher-grade data exploration tool",
        "environment": "local"
    },
    "apis": {
        "default": {
            "id": "default",
            "name": "KBase AppDev",
            "url": "http://localhost:8000",
            "type": "rest"
        },
        "test_data": {
            "id": "test_data",
            "name": "Local Test Data",
            "url": "http://localhost:8000",
            "type": "json_server"
        }
    },
    "dataTypes": {
        "genome_data_tables": {
            "configUrl": "/config/genome-data-tables.json",
            "matches": [
                "KBaseFBA.GenomeDataLakeTables-2.0",
                "KBaseFBA.GenomeDataLakeTables",
                "GenomeDataLakeTables"
            ],
            "autoLoad": true,
            "priority": 1
        },
        "pangenome": {
            "configUrl": "/config/pangenome.json",
            "matches": [
                "KBaseGenomes.Pangenome"
            ],
            "autoLoad": true
        },
        "test_data": {
            "configUrl": "/config/test-data.json",
            "matches": [
                "test_data"
            ],
            "autoLoad": true,
            "priority": 10
        }
    },
    "defaults": {
        "pageSize": 50,
        "theme": "light",
        "density": "default",
        "showRowNumbers": true,
        "locale": "en-US",
        "dateFormat": "YYYY-MM-DD"
    }
}